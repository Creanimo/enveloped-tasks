{
  "name": "five-server",
  "version": "0.0.14",
  "description": "Development Server with Live Reload Capability. (Maintained Fork of Live-Server)",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "bin": {
    "live-server": "./lib/bin.js",
    "five-server": "./lib/bin.js"
  },
  "keywords": [
    "browser",
    "development",
    "tool",
    "http",
    "live",
    "server",
    "cli",
    "preview",
    "reload",
    "refresh",
    "live-server",
    "live server"
  ],
  "scripts": {
    "depcheck": "npx depcheck",
    "start": "npm run dev",
    "dev": "npm run clean && tsc --watch",
    "build": "npm run extract && tsc && tsc --project tsconfig.injected.json",
    "clean": "rimraf lib",
    "prune": "rimraf .cache coverage lib node_modules package-lock.json",
    "format": "prettier --check src/**",
    "format:write": "prettier --write src/**",
    "lint": "eslint src --ext .ts,.tsx",
    "lint:fix": "eslint --fix src --ext .ts,.tsx",
    "extract": "extract version name src/const.ts",
    "test": "jest --forceExit --silent",
    "test:big": "npm run clean && npm run build && npm run format:write && npm run lint:fix && npm run test",
    "publish": "publish",
    "push": "npm run publish",
    "preReleaseHook": "clean build format:write lint:fix test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yandeu/five-server.git"
  },
  "author": "Yannick Deubel (https://github.com/yandeu)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/yandeu/five-server/issues"
  },
  "homepage": "https://github.com/yandeu/five-server#readme",
  "dependencies": {
    "accepts": "~1.3.7",
    "chokidar": "^3.5.1",
    "cors": "^2.8.5",
    "debug": "^4.3.1",
    "destroy": "~1.0.4",
    "encodeurl": "~1.0.2",
    "escape-html": "~1.0.3",
    "etag": "~1.8.1",
    "event-stream": "^4.0.1",
    "express": "^4.17.1",
    "fresh": "0.5.2",
    "mime": "^2.5.2",
    "mime-types": "~2.1.24",
    "morgan": "^1.9.1",
    "open": "8.0.4",
    "parseurl": "~1.3.3",
    "range-parser": "~1.2.1",
    "selfsigned": "^1.10.8",
    "ws": "^7.4.4"
  },
  "devDependencies": {
    "@actions/exec": "^1.0.4",
    "@types/express": "^4.17.11",
    "@types/mime": "^2.0.3",
    "@types/node": "^14.14.35",
    "@types/ws": "^7.4.0",
    "@yandeu/eslint-config": "0.0.1",
    "@yandeu/extract": "0.0.3",
    "@yandeu/prettier-config": "0.0.1",
    "eslint": "^7.22.0",
    "jest": "^26.6.3",
    "prettier": "^2.2.1",
    "publish-cli": "^1.7.0",
    "puppeteer": "^8.0.0",
    "rimraf": "^3.0.2",
    "supertest": "^6.1.3",
    "typescript": "^4.2.3"
  },
  "engines": {
    "node": ">=12"
  },
  "funding": {
    "url": "https://github.com/sponsors/yandeu"
  }
}